    #include<iostream>
    #include<cmath>
    using namespace std;
     
    int main(){
     
    	int arr[10][10],i,j,ans;
     
    	for(i=1; i<=5; i++){
    		for(j=1; j<=5; j++){
     
    			cin>>arr[i][j];
    		}
    	}
     
    	for(i=1; i<=5; i++){
    		for(j=1; j<=5; j++){
     
    			if(arr[i][j]==1){
    				ans = fabs(i-3)+fabs(j-3);
    				cout<<ans<<endl;
                    break;
    			}
     
    		}
    	}
     
    return 0;
     
    }
