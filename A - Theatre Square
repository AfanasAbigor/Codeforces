    #include<stdio.h>
    int main(){
    	
    	long long int n,m,a,n1,m1;
    	scanf("%lld %lld %lld", &n, &m, &a);
    	
    	 n1=n/a;
    	 m1=m/a;
    	 
    	 if(n%a!=0){
    	 	n1++;
    	 }
    	 
    	
    	 if(m%a!=0){
    	 	m1++;
    	 }
    	 
    	 
    	 printf("%lld\n",n1*m1);
    	
    	return 0;
    }
